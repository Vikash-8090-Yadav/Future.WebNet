        "product": {
          "name": "product",
          "type": "option",
          "summary": "Select a product for which to authenticate.",
          "multiple": false,
          "options": [
            "subgraph-studio",
            "hosted-service"
          ]
        },
        "studio": {
          "name": "studio",
          "type": "boolean",
