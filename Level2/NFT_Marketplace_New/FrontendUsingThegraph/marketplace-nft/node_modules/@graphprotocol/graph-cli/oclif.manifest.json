        },
        "skip-wait-for-postgres": {
          "name": "skip-wait-for-postgres",
          "type": "boolean",
          "summary": "Don't wait for Postgres to be up at localhost:15432",
          "allowNo": false
        },
        "timeout": {
          "name": "timeout",
          "type": "option",
          "summary": "Time to wait for service containers in milliseconds.",
          "multiple": false,
          "default": 120000
        }
      },
      "args": {
        "local-command": {
          "name": "local-command",
          "required": true
        }
      }
    },
